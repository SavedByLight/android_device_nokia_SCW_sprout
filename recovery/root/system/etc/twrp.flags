/system         ext4     system                                     flags=display="System";slotselect;backup=1
/system_ext     ext4     system_ext                                 flags=display="system_ext";slotselect;backup=1
/product        ext4     product                                    flags=display="Product";slotselect;backup=1
/vendor         ext4     vendor                                     flags=display="Vendor";slotselect;backup=1
/odm            ext4     odm                                        flags=display="odm";slotselect;backup=1
/metadata       ext4     /dev/block/bootdevice/by-name/metadata     flags=display="metadata";backup=1
/data           f2fs     /dev/block/bootdevice/by-name/userdata     flags=display="data";backup=1
/boot           emmc     /dev/block/bootdevice/by-name/boot         flags=display="Boot";flashimg=1;backup=1

/sdcard         vfat     /dev/block/mmcblk1p1                       flags=display="sdcard" 